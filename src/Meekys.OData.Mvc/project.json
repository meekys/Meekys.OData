{
    "name": "Meekys.OData.Mvc",
    "version": "0.0.1-*",
    "copyrigt": "Meekys (Sean Meechan)",

    "buildOptions": {
        "warningsAsErrors": true,
        "preserveCompilationContext": true,
        "debugType": "portable"
    },

    "dependencies": {
        "Microsoft.AspNetCore.Mvc": "1.0.0-*",
        "Meekys.Common": "*",
        "Meekys.OData": "*"
    },

    "frameworks": {
        "netcoreapp1.0": {
            "dependencies": {
                "Microsoft.NETCore.App": {
                    "type": "platform",
                    "version": "1.0.1"
                }
            },
            "imports": [
                "dotnet5.6",
                "dnxcore50",
                "portable-net45+win8"
            ]
        }
    },
  
    "tools": {},
  
    "tooling": {
        "defaultNamespace": "Meekys.OData.Mvc"
    },
  
    "packOptions": {
        "summary": "Mvc OData queryable extentions",
        "projectUrl": "https://github.com/meekys/Meekys.OData",
        "licenseUrl": "https://github.com/meekys/Meekys.OData/blob/master/LICENSE.md"
    }
}
